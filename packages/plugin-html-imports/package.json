{
  "name": "@prpl/plugin-html-imports",
  "version": "0.0.16",
  "description": "PRPL plugin that resolves HTML imports at build time",
  "author": "Ty Hopp (https://tyhopp.com)",
  "exports": {
    "./package.json": "./package.json",
    ".": [
      {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./dist/index.cjs.js"
    ]
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/packages/plugin-html-imports/src/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "static-site-generator",
    "ssg",
    "prpl",
    "plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@prpl/core": ">=0.0.69"
  },
  "gitHead": "ce6b428767bad4727b4d89ac979c6b4a4867c720"
}
