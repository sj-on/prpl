{
  "name": "prpl-test-site",
  "version": "0.0.1",
  "description": "PRPL site to run tests against",
  "scripts": {
    "dev:cjs": "rm -rf dist && node scripts/build.cjs && prpl-server",
    "build:cjs": "rm -rf dist && node scripts/build.cjs",
    "dev:mjs": "rm -rf dist && node scripts/build.mjs && prpl-server",
    "build:mjs": "rm -rf dist && node scripts/build.mjs",
    "test:open": "cypress open --project ..",
    "test": "cypress run --project .."
  },
  "dependencies": {
    "@prpl/core": "^0.2.2",
    "@prpl/plugin-code-highlight": "^0.2.2",
    "@prpl/plugin-css-imports": "^0.2.3",
    "@prpl/plugin-html-imports": "^0.2.3",
    "@prpl/plugin-rss": "^0.2.2",
    "@prpl/plugin-sitemap": "^0.1.3"
  },
  "devDependencies": {
    "@prpl/server": "^0.1.2",
    "cypress": "^8.2.0"
  }
}
